{
  "title": "Boogie",
  "name": "boogie",
  "version": "0.4.1",
  "description": "Debugging and logging utility for JavaScript.",
  "repository": {
    "type": "git",
    "url": "git@github.com:fczbkk/boogie.git"
  },
  "scripts": {
    "cleanup": "./node_modules/.bin/rimraf temp",
    "prebuild": "npm run cleanup",
    "build": "npm run build:test && npm run build:browser && npm run build:node",
    "build:browser": "./node_modules/.bin/webpack --config webpack.browser.config.js",
    "build:node": "./node_modules/.bin/webpack --config webpack.node.config.js",
    "build:test": "./node_modules/.bin/webpack --config webpack.test.config.js",
    "watch": "npm run watch:test & npm run watch:browser & npm run watch:node & npm run watch:karma",
    "watch:browser": "./node_modules/.bin/webpack --config webpack.browser.config.js --watch",
    "watch:node": "./node_modules/.bin/webpack --config webpack.node.config.js --watch",
    "watch:test": "./node_modules/.bin/webpack --config webpack.test.config.js --watch",
    "watch:karma": "./node_modules/.bin/karma start karma.conf.js",
    "dev": "npm run build && npm run watch",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/karma start karma.conf.js --single-run",
    "prerelease": "npm run build",
    "release": "./node_modules/.bin/bag-and-tag"
  },
  "author": "Riki Fridrich <riki@fczbkk.com> (https://github.com/fczbkk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczbkk/boogie/issues"
  },
  "homepage": "https://github.com/fczbkk/boogie#readme",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "bag-and-tag": "^1.0.2",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.13"
  },
  "main": "lib/node/boogie.js"
}
